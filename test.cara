fn nsieve(n) {
    var count = 0;
    var arr = [1; n];

    for i in (2, n - 1) {
        if arr[i] {
            count = count + 1;
            var j = i << 1;
            while j < n {
                arr[j] = 0;
                j = j + i;
            }
        }
    }

    print(n);
    print(count);
}

nsieve(5000000);
